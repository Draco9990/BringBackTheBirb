{
	"manifest_version": 3,
	"name": "Twitter Instead of X",
	"version": "1.0",
	"description": "Returns all X logos back to their Twitter design.",
	"content_scripts": [
		{
		  "js": ["jquery.js", "scripts/twitter-main.js"],
		  "matches": [
				"https://twitter.com/*"
		  ]
		}
	],
	"web_accessible_resources":[
		{
		  "resources": [ "images/betterResults.png", "images/notmrbeast.png", "images/blockedcontent.png", "images/char/mrbeast/walk0.png", "images/char/mrbeast/walk1.png", "images/char/mrbeast/walk2.png", "images/char/mrbeast/walk3.png", "images/char/mrbeast/drag0.png", "images/char/mrbeast/drag1.png"],
		  "matches": [ "<all_urls>" ]
		}
	],
	"icons": {
		"16": "images/icon-16.png",
		"32": "images/icon-32.png",
		"48": "images/icon-48.png",
		"128": "images/icon-128.png"
	 }
}